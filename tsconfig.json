{
  "compilerOptions": {
    "lib": ["es2017"],
    "jsx": "react-native",
    "noResolve": false,
    "noImplicitAny": false,
    "removeComments": false,
    "sourceMap": false,
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es5",
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "outDir": "lib",
    "baseUrl": "./",
    "paths": {
      "@react-navigation/stack": [
        "./example/node_modules/@react-navigation/stack"
      ],
      "@react-navigation/native": [
        "./example/node_modules/@react-navigation/native"
      ]
    }
  },
  "compileOnSave": true,
  "exclude": ["node_modules"],
  "include": ["./index.tsx", "./src/*"]
}
